{
    "name": "@goodt/postcss-config",
    "version": "2.2.0",
    "description": "SUP: Postcss. Shared postcss configs and package dependencies config for reuse in SUP projects.",
    "publishConfig": {
        "registry": "https://art.goodt.me/repository/npm/"
    },
    "scripts": {
        "prepare1": "echo module.exports = > ./tw-ignored-classes.js && list-selectors ./../../node_modules/goodt-framework-css/dist/all.css -p -i classes >> ./tw-ignored-classes.js",
        "postinstall1": "echo module.exports = > ./tw-ignored-classes.js && list-selectors ./../../goodt-framework-css/dist/all.css -p -i classes >> ./tw-ignored-classes.js",
        "release": "npm_config_registry=https://registry.npmjs.org npx standard-version --skip.changelog",
        "release:patch": "npm run release -- --release-as patch"
    },
    "dependencies": {
        "@fullhuman/postcss-purgecss": "^3.0.0",
        "autoprefixer": "^10.0.0",
        "list-selectors": "^2.0.0",
        "postcss": "^8.0.0",
        "postcss-advanced-variables": "^3.0.1",
        "postcss-bem-fix": "^2.1.0",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^6.0.0",
        "postcss-nested": "^4.0.0",
        "postcss-plugin-px2rem": "^0.8.1",
        "postcss-remove-declaration": "^1.0.0",
        "postcss-wrap": "^0.0.4",
        "tailwindcss": "^2.0.0"
    }
}

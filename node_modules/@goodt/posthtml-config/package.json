{
  "name": "@goodt/posthtml-config",
  "description": "SUP: Linting. Shared posthtml configs and package dependencies config for reuse in SUP projects.",
  "version": "1.1.1",
  "publishConfig": {
    "registry": "https://art.goodt.me/repository/npm/"
  },
  "main": "./index.js",
  "scripts": {
    "postinstall": "cd ./../../../ && npx patch-package --patch-dir=./node_modules/@goodt/posthtml-config/.patches || echo 'Patch failed'",
    "release": "npm_config_registry=https://registry.npmjs.org npx standard-version --skip.changelog",
    "release:patch": "npm run release -- --release-as patch"
  },
  "dependencies": {
    "posthtml": "^0.16.5",
    "posthtml-bemify": "^1.0.4",
    "posthtml-loader": "^2.0.1"
  }
}
